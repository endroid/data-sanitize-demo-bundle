{
    "name": "endroid/data-sanitize-demo-bundle",
    "description": "Endroid Data Sanitize Demo Bundle",
    "keywords": ["endroid", "data", "sanitize", "demo", "symfony", "bundle", "php"],
    "homepage": "https://github.com/endroid/data-sanitize-demo-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl"
        }
    ],
    "require": {
        "php": "^8.1",
        "endroid/data-sanitize-bundle": "^1.2.7",
        "endroid/installer": "^1.2.2"
    },
    "require-dev": {
        "endroid/quality": "dev-master"
    },
    "suggest": {
        "roave/security-advisories": "Avoids installation of package versions with vulnerabilities"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\DataSanitizeDemoBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\DataSanitizeDemoBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "endroid/installer": true
        }
    }
}
